{
  "name": "portfolio",
  "version": "2.0.0",
  "description": "Portfolio",
  "main": "index.js",
  "homepage": "https://aliciafdezrov.github.io/portfolio",
  "sideEffects": false,
  "scripts": {
    "start": "run-p -l type-check:watch start:dev",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "start:dev": "webpack serve --config ./config/webpack/dev.js",
    "start:prod": "webpack serve --config ./config/webpack/prod.js",
    "build:dev": "rimraf dist && npm run type-check && webpack --config ./config/webpack/dev.js",
    "build:prod": "rimraf dist && npm run type-check && webpack --config ./config/webpack/prod.js",
    "build:perf": "rimraf dist && npm run type-check && webpack --config ./config/webpack/perf.js",
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aliciafdezrov/portfolio"
  },
  "author": "Alicia Fern√°ndez Rovira",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@rsuite/icons": "1.0.2",
    "react": "17.0.2",
    "react-device-detect": "1.9.10",
    "react-dom": "17.0.2",
    "react-genie": "^0.1.4",
    "react-toastify": "5.4.0",
    "regenerator-runtime": "0.13.9",
    "rsuite": "5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.8",
    "@babel/preset-env": "7.14.9",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.9",
    "@types/styled-components": "5.1.15",
    "babel-loader": "8.2.2",
    "babel-preset-rsuite": "4.0.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "5.0.1",
    "gh-pages": "^3.2.3",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.2",
    "less": "4.1.2",
